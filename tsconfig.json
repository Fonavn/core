{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@app/fona": [
        "apps/fona/src"
      ],
      "@app/fona/*": [
        "apps/fona/src/*"
      ],
      "@lib/auth": [
        "libs/auth/src"
      ],
      "@lib/auth/*": [
        "libs/auth/src/*"
      ],
      "@lib/core": [
        "libs/core/src"
      ],
      "@lib/core/*": [
        "libs/core/src/*"
      ],
      "@lib/tenant": [
        "libs/tenant/src"
      ],
      "@lib/tenant/*": [
        "libs/tenant/src/*"
      ],
      "@lib/shared": [
        "libs/shared/src"
      ],
      "@lib/shared/*": [
        "libs/shared/src/*"
      ],
      "@lib/mail": [
        "libs/mail/src"
      ],
      "@lib/mail/*": [
        "libs/mail/src/*"
      ],
      "@lib/photo": [
        "libs/photo/src"
      ],
      "@lib/photo/*": [
        "libs/photo/src/*"
      ],
      "@lib/storage": [
        "libs/storage/src"
      ],
      "@lib/storage/*": [
        "libs/storage/src/*"
      ],
      "@lib/facebook": [
        "libs/facebook/src"
      ],
      "@lib/facebook/*": [
        "libs/facebook/src/*"
      ],
      "@lib/crm": [
        "libs/crm/src"
      ],
      "@lib/crm/*": [
        "libs/crm/src/*"
      ],
      "@lib/scm": [
        "libs/scm/src"
      ],
      "@lib/scm/*": [
        "libs/scm/src/*"
      ],
      "@lib/promotion": [
        "libs/promotion/src"
      ],
      "@lib/promotion/*": [
        "libs/promotion/src/*"
      ]
    }
  }
}